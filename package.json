    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "serve": "serve -s dist",
    "start": "npm run dev",
    "test": "vitest --run --no-ui --no-open --no-coverage",
    "test-ui": "vitest --ui --open ---no-coverage",
    "lint": "eslint --flag unstable_ts_config --cache --cache-strategy content",
    "lint-fix": "eslint --flag unstable_ts_config --fix",
